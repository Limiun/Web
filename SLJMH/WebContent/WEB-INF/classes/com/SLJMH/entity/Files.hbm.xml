<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<!-- 建立类与表的映射 -->
	<class name="com.SLJMH.entity.Files" table="files">
		<id name="fileId" column="fileId">
			<generator class="identity"/>
		</id>
		<!-- 普通属性 -->
		<property name="fileName" column="fileName" type="string"/>
		<property name="fileUuuName" column="fileUuuName" type="string"/>
		<property name="filePath" column="filePath"/>
		<property name="fileUrl" column="fileUrl" type="string"/>
		<property name="fileType" column="fileType" type="string"/>
		<property name="fileuserName" column="fileuserName" type="string"/>
		<property name="filealbumName" column="filealbumName" type="string"/>
	</class>
	

</hibernate-mapping>
